<h1>Welcome to the Tech Blog!</h1>
{{#if loggedIn}}
<a href="/dashboard">Go to Dashboard</a>
<a href="/logout">Logout</a>
{{else}}
<a href="/login">Login</a>
<a href="/signup">Sign Up</a>
{{/if}}
{{#each posts}}
<div>
    <h2>{{this.title}}</h2>
    <p>{{this.content}}</p>
    <p>By {{this.username}} on {{this.createdAt}}</p>
</div>
{{/each}}
